<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

          <bean id="emp" class="com.manish.spring.domain.Employee">
              <property name="eid" value="1001"/>
              <property name="ename" value="Kumar"/>
          </bean>

             <bean id="e2" class="com.manish.spring.domain.Employee" primary="true">
                 <property name="eid" value="1002"/>
                 <property name="ename" value="Kishore"/>
             </bean>
</beans>